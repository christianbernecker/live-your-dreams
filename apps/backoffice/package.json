{
  "name": "lyd-backoffice-v2",
  "version": "2.0.0",
  "private": true,
  "description": "Live Your Dreams Backoffice - Design System Clean Implementation",
  "scripts": {
    "dev": "NEXT_TELEMETRY_DISABLED=1 next dev --port 3001 --turbo",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "NEXT_TELEMETRY_DISABLED=1 next start --port 3001",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next",
    "analyze": "ANALYZE=true npm run build",
    "deploy": "vercel --prod",
    "deploy-preview": "vercel",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "next": "14.2.33",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "next-auth": "5.0.0-beta.22",
    "bcryptjs": "2.4.3",
    "@prisma/client": "5.19.1",
    "zod": "3.22.4",
    "@vercel/blob": "^0.23.4",
    "react-dropzone": "^14.2.3"
  },
  "devDependencies": {
    "@types/node": "20.16.5",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@types/bcryptjs": "2.4.6",
    "prisma": "5.19.1",
    "typescript": "5.5.4",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.33",
    "tsx": "4.19.1",
    "@types/react-dropzone": "^5.1.0"
  }
}